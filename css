.filter(([key]) => {
  if (!backendFilterKey) return true;

  // Match more flexibly: either exact or contains backendKey
  return key === backendFilterKey || key.includes(backendFilterKey);
})





.filter(r =>
  !backendFilterKey
    ? true
    : r.PartitionName2 === backendFilterKey || r.PartitionName2.includes(backendFilterKey)
)