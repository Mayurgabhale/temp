
const partitionRows = useMemo(() => {
  if (!summaryEntry || !summaryEntry.partitions) return [];

  return Object.entries(summaryEntry.partitions)
    .filter(([key]) => {
      if (!key || typeof key !== 'string') return false;
      if (!decodedPartition) return true;

      const normalizedKey = key.toLowerCase();
      const normalizedPartition = decodedPartition.toLowerCase();

      return (
        normalizedKey === normalizedPartition ||
        normalizedKey.endsWith(`.${normalizedPartition}`) ||
        normalizedKey.includes(normalizedPartition)
      );
    })
    .map(([key, value]) => ({
      ...value,
      key,
      total: value.employee + value.contractor,
    }));
}, [summaryEntry, decodedPartition]);








