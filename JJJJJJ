function getDeviceTypeIcon(type) {
    switch (type) {
        case 'cameras': return 'fa-camera';
        case 'controllers': return 'fa-microchip';
        case 'archivers': return 'fa-database';
        case 'servers': return 'fa-server';
        default: return 'fa-box'; // default icon
    }
}
