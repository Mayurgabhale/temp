// Vilnius â†’ GAMA building only (by floor)
const allFloors = data?.realtime?.['LT.Vilnius']?.floors || {};
const vilniusFloors = Object.entries(allFloors)
  .filter(([name]) => name.toUpperCase().includes('GAMA'))  // ðŸ§  Keep only GAMA
  .map(([name, headcount]) => ({
    name: name.replace('GAMA - ', ''), // Optional: cleaner label
    headcount,
    capacity: 50  // Optional: set real capacity per floor if you have it
  }));