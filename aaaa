const todayData = data?.today || {};
const realtimeData = data?.realtime || {};

const todayTot = todayData.total || 0;
const todayEmp = todayData.Employee || 0;
const todayCon = todayData.Contractor || 0;

const realtimeTot = Object.values(realtimeData).reduce((s, x) => s + (x?.total || 0), 0);
const realtimeEmp = Object.values(realtimeData).reduce((s, x) => s + (x?.Employee || 0), 0);
const realtimeCon = Object.values(realtimeData).reduce((s, x) => s + (x?.Contractor || 0), 0);