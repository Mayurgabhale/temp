const cityGroup = ['Dublin', 'Rome', 'Moscow'];
const combinedData = summaryItems
  .filter(item => cityGroup.includes(item.label))
  .reduce(
    (acc, item) => {
      acc.total += item.total || 0;
      acc.emp += item.emp || 0;
      acc.cont += item.cont || 0;
      return acc;
    },
    { total: 0, emp: 0, cont: 0 }
  );